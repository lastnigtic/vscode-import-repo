{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES2015",
    "module": "ESNext",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "jsx": "react",
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true
  }
}
